{
  "name": "duck",
  "version": "1.0.0",
  "description": "",
  "main": "script.js",
  "productName": "Duck",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "spago build --purs-args \"-g sourcemaps\"",
    "test": "spago -x test.dhall test  --purs-args \"-g sourcemaps\"",
    "esbuild-renderer": "esbuild --sourcemap renderer.mjs --bundle --outfile=renderer.js",
    "esbuild-script": "esbuild --sourcemap main.mjs --bundle --platform=node --target=node16.15 --external:electron --outfile=script.js",
    "esbuild": "npm run esbuild-renderer && npm run esbuild-script",
    "just-start": "electron .",
    "prebundle": "npm run build && npm run esbuild",
    "bundle": "electron-packager --overwrite .",
    "prestart": "npm run build && npm run esbuild",
    "start": "npm run just-start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.3",
    "@hpcc-js/wasm": "^1.14.1",
    "@petamoriken/float16": "^3.6.5",
    "@storybook/builder-webpack5": "next",
    "@storybook/manager-webpack5": "next",
    "big-integer": "^1.6.51",
    "framer-motion": "^6.3.16",
    "nearley": "^2.20.1",
    "node-fetch": "^3.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^2.16.1",
    "sjcl": "github:athanclark/sjcl#e6ca43fbcc85689f9e6b212cc88b85a53295a459",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@react-aria/overlays": "^3.9.1",
    "@storybook/react": "next",
    "babel-loader": "^8.2.5",
    "electron": "^19.0.7",
    "electron-packager": "^15.5.1",
    "esbuild": "^0.14.48",
    "global-jsdom": "^8.5.0"
  }
}